numero, numero2 = input().split()
numero = int(numero)
numero2 = int(numero2)

if 0 < numero <= 50 and 0 < numero2 <= 50:
   print("R1", end = "")
elif -50 <= numero < 0 and 0 < numero2 <= 50:
    print("R2", end = "")
elif -50 <= numero < 0 and -50 <= numero2 < 0:
    print("R3", end = "")
elif 0 < numero <= 50 and -50 <= numero2 < 0:
    print("R4", end = "")
elif numero == 0 and numero2 == 0:
    print("NO ALVO!", end = "")
